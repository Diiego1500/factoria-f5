{{ form_start(form, { 'attr': { 'class': 'needs-validation', 'novalidate': 'novalidate', 'id': 'formSendImages' } }) }}
    <div class="mb-3">
        {{ form_label(form.title, 'Titulo*', { 'label_attr': { 'class': 'form-label' } }) }}
        {{ form_widget(form.title, { 'attr': { 'class': 'form-control', 'placeholder': 'Enter title', 'required': 'required' } }) }}
        <div class="invalid-feedback">Please provide a title.</div>
        {{ form_errors(form.title) }}
    </div>
    <div class="mb-3">
        {{ form_label(form.description, 'Descripción', { 'label_attr': { 'class': 'form-label' } }) }}
        {{ form_widget(form.description, { 'attr': { 'class': 'form-control', 'placeholder': 'Enter description' } }) }}
        {{ form_errors(form.description) }}
    </div>
    <div class="mb-3">
        {{ form_label(form.filename, 'Imágen*', { 'label_attr': { 'class': 'form-label' } }) }}
        {{ form_widget(form.filename, { 'attr': { 'class': 'form-control' } }) }}
        {{ form_errors(form.filename) }}
    </div>
    <div class="d-grid">
        <button type="submit" class="btn btn-success" id="btnSubmitForm">Cargar Imágen</button>
    </div>
{{ form_end(form) }}